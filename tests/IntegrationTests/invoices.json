				{
					"name": "Register Client Admin",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "2801ef8e-ce42-431a-85ee-0d696fee2789",
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Response body contains expected fields\", function () {\r",
									"    const responseData = pm.response.json();\r",
									"\r",
									"    pm.expect(responseData).to.be.an('object');\r",
									"    pm.expect(responseData.id).to.exist;\r",
									"    pm.expect(response.firstName).to.exist;\r",
									"    pm.expect(response.lastName).to.exist;\r",
									"    pm.expect(response.department).to.exist;\r",
									"    pm.execution(response.email).to.exist;\r",
									"    pm.execution(response.confirmEmailToken).to.exist;\r",
									"\r",
									"    pm.environment.set(\"clientUserId\", responseData.id);\r",
									"    pm.environment.set(\"email\", responseData.email);\r",
									"    pm.environment.set(\"confirmEmailToken\", responseData.confirmEmailToken);\r",
									"});\r",
									"\r",
									"pm.test(\"Response body contains expected values\", function (){\r",
									"    const responseData = pm.response.json();\r",
									"    const requestData = pm.request.json();\r",
									"\r",
									"    pm.expect(responseData.firstName).to.equal(requestData.firstName);\r",
									"    pm.expect(responseData.lastName).to.equal(requestData.lastName);\r",
									"    pm.expect(responseData.department).to.equal(responseData.department);\r",
									"    pm.expect(responseData.email).to.equal(responseData.email);\r",
									"    pm.expect(responseData.confirmEmailToken).to.equal(responseData.confirmEmailToken);\r",
									"\r",
									"    pm.environment.set(\"password\", requestData.password);\r",
									"});\r",
									"\r",
									"\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
